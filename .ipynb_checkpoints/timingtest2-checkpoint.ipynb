   "outputs": [],
   "source": [
    "a = np.array(range(1,10000000))\n",
    "a"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "b = np.zeros(len(a),dtype=int)\n",
    "b"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "length = len(a)\n",
    "for i in range(0, length - 1):\n",
    "    b[i] = a[i] + a[i + 1]\n",
    "b[length - 1] = a[length - 1] + a[0]\n",
    "b"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "a + np.append(a[1:],a[0])"
   ]
  }
